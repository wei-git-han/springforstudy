<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.dao.UserMapper">
    <select id="queryUserList" resultType="com.entity.User">
    select * from han
    </select>
    <select id="queryUserById" resultType="com.entity.User">
        select * from han where id=#{id}
    </select>

</mapper>