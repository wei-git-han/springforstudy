<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="project.dao.UserDao">
    <resultMap id="result" type="project.entity.User">
        <result property="id" column="id"></result>
        <result property="name" column="name"></result>
        <result property="deptId" column="deptId"></result>
    </resultMap>
    <select id="getList" parameterType="string" resultType="project.entity.User">
        select * from user_han where id = #{id}
    </select>
</mapper>